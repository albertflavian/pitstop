{
  "RabbitMQHandler": {
    "Host": "rabbitmq-service",
    "Username": "guest",
    "Password": "guest",
    "Exchange": "Pitstop",
    "Queue": "Auditlog",
    "RoutingKey": ""
  },
  "Auditlog": {
    "path": "/var/log/pitstop/auditlog"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Information",
    "Properties": {
      "Application": "AuditlogService"
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://logserver:5341" }
      }
    ]
  }
}
